{
    "name": "aleksandrx/laravel-hmrc-api",
    "type": "library",
    "require": {
        "darkside666/hmrc-api-php": "develop"
    },
    "require-dev": {
        "orchestra/testbench": "^3.7",
        "phpunit/phpunit": "^9.5.25 || ^10.0 || ^11.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Patompong Savaengsuk",
            "email": "p.pangpond@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "LaravelHMRC\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LaravelHMRC\\Test\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaravelHMRC\\ServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    }
}
